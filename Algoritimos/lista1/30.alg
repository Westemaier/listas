algoritmo "30"
var
   RS, US, cotacao: real

inicio
      escreva("Digite um numero para o real: ")
      leia(RS)
      escreva("Digite um valor para cotacao do dolar: ")
      leia(cotacao)
      US <- RS * cotacao
      escreva("O valor do dolar correspodente eh ", US)
      
      
fimalgoritmo
