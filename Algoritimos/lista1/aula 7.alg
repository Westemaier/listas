algoritmo "Viagem para disney"
var
   ano, nasc, idade: inteiro


inicio
      escreva("em que ano estamos? ")
      leia(ano)
      escreva("Em que ano voce nasceu? ")
      leia(nasc)
      idade <- ano - nasc
      escreva ("Em ", ano, " voce tera ", idade, " anos.")
      se (idade >= 21) entao
      escreval (" E ja tera atingido a maior idade." )
      Fimse
fimalgoritmo
